		p.x=0
		p.y=0
		m=lvl[level].m
		l=lvl[level].l
		enemies=lvl[level].enemies
	end
end

function completion()
	local rem=0
	for level in all(lvl) do
		rem+=#level.enemies-1
	end
	return rem
end
