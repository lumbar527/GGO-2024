			if segmentvscircle(p1.x,p1.y,p2.x,p2.y,xx,yy,10) then
				return false,thing
			end
		end
	end
	return true
end

function bmove(xx,yy)
	for thing in all(l) do
		p1=m[thing[1]]
		p2=m[thing[2]]
