function tri(x1,y1,x2,y2,x3,y3,c,p)
	local ux,uy=x2,y2
	local sx=(x3-x2)/p
	local sy=(y3-y2)/p
	for i=1,p do
		line(x1,y1,ux+i*sx,uy+i*sy,c)
	end
	ux,uy=x1,y1
	sx=(x3-x1)/p
	sy=(y3-y1)/p
	for i=1,p do
		line(x2,y2,ux+i*sx,uy+i*sy,c)
	end
	ux,uy=x2,y2
	sx=(x1-x2)/p
	sy=(y1-y2)/p
	for i=1,p do
		line(x3,y3,ux+i*sx,uy+i*sy,c)
	end
end
