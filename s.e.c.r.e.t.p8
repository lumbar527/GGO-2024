function can_move(xx,yy,en)
	for thing in all(l) do
		if thing[1]!=en then
			p1=m[thing[1]]
			p2=m[thing[2]]
	--		if do_intersect(p1.x,p1.y,p2.x,p2.y,p.x+sin(p.a)*10,p.y+cos(p.a)*10,xx+sin(p.a)*10,yy+cos(p.a)*10)[1] then
